#include <iostream>
#include <vector>
#include <string>

std::vector<std::string> reverseStrings(std::vector<std::string>string) {
    std::vector<std::string> reversed;
    for(std::string s : strings) {
        std::string rev="";
        for(int i= s.lenght() -1; i>= 0; i--){
            rev+= s[i]
        }
        reversed.push_back(rev);
    }
    return reversed
}

int main() {
    std::vector<std::string> reversed = reverseStrings({"abc","def"});
    for(std::string s : reversed)std::cout<<s<<" "; //output: cba fed
    return 0;
}