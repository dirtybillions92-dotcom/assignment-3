#include <iostream>
#include <vector>

std::vector<int>removeEvens(std::vector<int>nums) {
    std::vector<int>odds;
    for(int num:nums) {
        if(num%2!=0) {
            odd.push_back(num);
    }
    }
    return odd;
}
int main() {
    std::vector<int>odd= removeEvens({1,2,3,4,5});
    for(int num: odds) std::cout << num << ""; //output:1 3 5
    return 0;
}